{
  "extends": [
    "eslint:recommended", 
    "plugin:import/errors", 
    "plugin:react/recommended", 
    "prettier", 
    "prettier/react"],
  "plugins": ["react", "react-hooks", "import", "jsx-a11y"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
    "rules": {
		"react-hooks/rules-of-hooks": 2 ,
    "react-hooks/exhaustive-deps": 1,
    "react/prop-types": 0,
    "no-console": 2 
    },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}